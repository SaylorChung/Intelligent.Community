@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <script src="~/Scripts/loading.js"></script>
    <!--CSS start-->
    <link href="~/Scripts/jquery-easyui/themes/metro/easyui.css" rel="stylesheet" />
    <link href="~/Scripts/jquery-easyui/themes/icon.css" rel="stylesheet" />
    <link href="~/Scripts/jquery-easyui/themes/color.css" rel="stylesheet" />
    <link href="~/assets/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <!--CSS end-->
    <!--Scripts start-->
    <script src="~/Scripts/jquery/jquery-1.11.3.min.js"></script>
    <script src="~/Scripts/jquery-easyui/jquery.easyui.min.js"></script>
    <script src="~/assets/scroll/jquery.nicescroll.js"></script>
    <script src="~/Scripts/jquery-easyui/locale/easyui-lang-zh_CN.js"></script>
    <script src="~/Scripts/pages/index.js"></script>
    <title>路政基础信息管理系统</title>
</head>
<body class="easyui-layout">
    <div data-options="region:'north',border:true" style="height:50px;background-color:#271f1f">
        <div class="sidebar-toggle-box" style="color:#CA552F;background:#2D2728;">
            <i class="fa fa-bars tooltips" data-placement="right"></i>
        </div>
        <a href="/" style="float:left;text-decoration:none;">
            <img style="margin-top:10px;margin-left:4px; float:left;" alt="logo" src="~/images/logo.png">
            <span style="color:white;cursor:pointer;line-height:48px;">&nbsp;&nbsp;路政巡查基础信息管理系统</span>
            <sup style="color:white;">2.0</sup>
        </a>
        <div id="top_links">
            <ul>
                <li style="padding-left:15px;">
                    <span style="color:white;cursor:pointer;">您好！@User.Identity.Name</span>
                </li>
                <li style="padding-right:15px;padding-left:10px;">
                    <i class="fa fa-hand-o-right"></i>
                    <a href="/Account/LogOff">
                        <span style="color:white;cursor:pointer;">退出</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div data-options="region:'west',split:false,iconCls:'fa fa-dashboard'" title="导航面板"
         style="width: 180px; height: 99%; padding: 10px;" id="leftContent">
        <div id="leftAccordion" class="easyui-accordion"
             style="margin-left: -10px; margin-top: -10px; width:178px; border-top:0px; border-right:0px;border-bottom:0px;">
            @{ Html.RenderAction("Accordion", "Partial");}
            @*<div title="日历" data-options="selected:true,collapsible:false,iconCls:'fa fa-calendar'">
                    <div class="easyui-calendar" style="width:177px;height:160px;"></div>
                </div>*@
        </div>
        <div style="height:34px;"></div>
    </div>
    <div data-options="region:'south',border:false">
        <div id="footer">
            2015 © 常州迈普网络科技有限公司。
        </div>
    </div>
    <div data-options="region:'center',border:false" id="Content">
        <iframe scrolling="no" id="pageContent" frameborder="0" style="width:100%;height:99%; overflow:hidden;"></iframe>
    </div>
</body>
</html>
